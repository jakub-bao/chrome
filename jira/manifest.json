{
  "manifest_version": 3,
  "name": "Get JIRA ticket",
  "description": "Fill timesheet",
  "version": "0.0.1",
  "content_scripts": [
    {
      "js": ["build/index.js"],
      "matches": [
        "https://jira.pepfar.net/browse/*"
      ],
      "type": "module"
    }
  ],
  "permissions" : [
    "contextMenus",
    "clipboardWrite"
  ]
}